{
  "name": "slack-clone-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm css:build && rsbuild build",
    "check": "biome check --write",
    "format": "biome format --write",
    "js:dev": "rsbuild dev --open",
    "css": "tailwindcss -i ./frontend/src/global.css -o ./frontend/src/global.min.css",
    "css:dev": "pnpm css --watch",
    "css:build": "pnpm css --minify",
    "preview": "rsbuild preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.11",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@rsbuild/core": "^1.4.3",
    "@rsbuild/plugin-react": "^1.3.3",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "typescript": "^5.8.3"
  }
}
